{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "QuantumNEC - Build and debug Kernel",
      "type": "cppdbg",
      "request": "launch",
      "preLaunchTask": "Build and Run Kernel",
      "miDebuggerServerAddress": "localhost:1234",
      "program": "${workspaceFolder}/build/Kernel/Kernel.elf",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "targetArchitecture": "amd64",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/QuantumNEC/QuantumOS/QuantumNEC",
      "program": "/home/QuantumNEC/QuantumOS/QuantumNEC/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
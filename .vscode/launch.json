{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "QuantumNEC - Build and debug Kernel",
      "type": "cppdbg",
      "request": "launch",
      "preLaunchTask": "Build and Run Kernel",
      "miDebuggerServerAddress": "localhost:1234",
      "program": "${workspaceFolder}/build/esp/QuantumNEC/microKernel.elf",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "targetArchitecture": "x86_64",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}